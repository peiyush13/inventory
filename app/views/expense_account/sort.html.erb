<table class="table table-hover" id="table_body">
    <thead>
    <tr>
        <th>Expense ID</th>
        <th>Expense Type</th>
        <th>Description</th>
        <th>Cost</th>
        <th>Date</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    </thead>

    <tbody>
    <% @expenses.each  do  |expense| %>
    <tr>
        <td><%= expense.e_id %></td>
        <td><%= expense.title %></td>
        <td><%= expense.description %></td>
        <td><%= expense.cost %></td>
        <td><%= expense.dated %></td>
        <td><%= link_to('Edit',:action =>'update',:e_id=>expense.e_id) %>
        </td>
        <td><%= link_to('delete',:action =>'delete',:e_id=> expense.e_id) %>
        </td>
    </tr>
    <% end %>
    </tbody>
</table>